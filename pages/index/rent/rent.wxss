.container{
  padding: 0;
  margin: 0;
  background-color: #efefef;
}
.line{
  display: inline-flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 5rem;
  margin-top: 0.5rem;
  background-color: #fff;  
}
.line:nth-of-type(1){
  margin-top: 0;
}
.Img{
  display: inline-flex;
  align-items: center;
  justify-content: space-between;
  width: 25%;
  height: 5rem;
}
.Img image{
  width: 4rem;
  height: 4rem;
}
.Name{
  width: 35%;
  height: 4rem;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.all{
  width: 30%;
  height: 4rem;
}
.see,.money{
  width: 100%;
  height: 2rem;
  font-size: 0.8rem;
  color: #8a8a8a;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}