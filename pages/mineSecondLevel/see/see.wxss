.container{
  padding: 0;
  margin: 0;
  background-color: #efefef;
}

.see-con{
  float: left;
  width: 100%;
  height: 4rem;
  background-color: #fff;
}
.see-tab-left{
  float: left;
  width: 30%;
  height: 4rem;
}
.numLeft,.numRight{
  display: block;
  width: 100%;
  height: 2rem;
  line-height: 2rem;
  text-align: center;
  color: #8a8a8a;
  font-size: 1.5rem;
}
.numLeftText,.numRightText{
  display: block;
  width: 100%;
  height: 1.5rem;
  line-height: 1.5rem;
  text-align: center;
  font-size: 0.9rem;
}
.see-tab-right{
  float: right;
  width: 30%;
  height: 4rem;
}
.jyImg{
  margin-top: 5rem;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  width: 100%;
}
.jyImg image{
  width: 8rem;
  height: 8rem;
}
.jyText{
  width: 70%;
}