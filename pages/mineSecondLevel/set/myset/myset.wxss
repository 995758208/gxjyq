.container{
  padding: 0;
  margin: 0;
  background-color: #efefef;
}
.header{
  margin-top: 1px;
  width: 100%;
  height: 4rem;
  background-color: #fff;
}
.end{
  display: inline-flex;
  justify-content: space-between;
  margin-left: 2.5%;
  width: 95%;
  height: 4rem;
  background-color: #fff;
  border-bottom:1px solid #efefef;
}
.headerText{
  width: 20%;
  height: 4rem;
  line-height: 4rem;
  padding-left: 5%;
  font-size: 1rem;
}
.headerText2{
  width: 20%;
  height: 4rem;
  line-height: 4rem;
  padding-left: 5%;
  color: #8a8a8a;
  font-size: 0.8rem;
  text-align: center;
}
.headerImg{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding-right: 5%;
  width: 80%;
  height: 4rem;
}
.headerImg image{
  width: 3rem;
  height: 3rem;
}