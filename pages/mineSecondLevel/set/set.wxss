.container{
  padding: 0;
  margin: 0;
  background: #efefef;
}
.sets{
  display: inline-flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 3rem;
  line-height: 3rem;
  background-color: #fff;
}
.sets text{
  padding-left: 0.5rem;
  display: inline-flex;
  width: 80%;
  color: #8a8a8a;
}
.sets image{
  padding-right: 0.5rem;
  width: 1.5rem;
  height: 1.5rem;
}